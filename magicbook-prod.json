{
  "title": "Code of Music",
  "addPlugins": ["magicbook-codesplit", "magicbook-webpack", "./plugins/p5"],
  "files": [
    "static-docs/index.html",
    {
      "label": "Chapters",
      "files": ["notion-docs/**/index.md"]
    },
    {
      "images": {
        "files": "images/**/*.png"
      }
    }
  ],
  "stylesheets": {
    "bundle": true,
    "compress": true,
    "digest": true,
    "files": ["stylesheets/html.scss"]
  },
  "javascripts": {
    "files": ["javascripts/main.js"],
    "bundle": true
  },
  "fonts": {
    "files": "fonts/**/*.*"
  },
  "webpack": "webpack.config.js",
  "builds": [
    {
      "format": "html",
      "destination": "build/build-prod/web"
    },
    {
      "format": "pdf",
      "destination": "build/build-prod/pdf"
    }
  ],
  "codesplit": {
    "includes": "examples"
  },
  "layout": "layouts/default.html"
}
