{
  "name": "mbcodeofmusic",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "rm -rf build/build-dev && magicbook build --watch --config=magicbook-dev.json",
    "serve": "live-server build/build-dev/web --port=5000",
    "build:prod": "rm -rf build/build-prod && magicbook build --config=magicbook-prod.json && yarn copy-pdf",
    "copy-pdf": "cp build/build-prod/pdf/consolidated.pdf build/build-prod/web/the-code-of-music.pdf",
    "move-images": "find ./notion-docs -name '*.png' -exec sudo mv '{}' ./images/ \\;",
    "post-notion-import": "yarn move-images && sudo node scripts/post-notion-import.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "magicbook": "^0.1.19",
    "magicbook-codesplit": "^0.1.6",
    "magicbook-webpack": "0.0.4",
    "p5": "^1.3.1",
    "path": "^0.12.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "replace-in-file": "^6.2.0",
    "roughjs": "^4.4.1",
    "source-map-loader": "^3.0.0",
    "tinyliquid": "^0.2.34",
    "tone": "^14.7.77",
    "webpack": "^4.42.1"
  },
  "devDependencies": {
    "firebase-tools": "^9.16.0",
    "live-server": "^1.2.1"
  }
