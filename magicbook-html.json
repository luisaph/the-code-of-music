{
  "title": "Code of Music",
  "addPlugins": ["magicbook-codesplit", "magicbook-webpack", "./plugins/p5"],
  "files": ["notion-docs-2/**/*.md"],
  "images": {
    "files": ["notion-images/**/*.png", "images/**/*.png", "examples/img/*.*"],
    "destination": "assets/img/"
  },
  "stylesheets": {
    "bundle": true,
    "compress": true,
    "digest": true,
    "files": ["stylesheets/html.scss", "stylesheets/highlightjs.css"]
  },
  "javascripts": {
    "files": ["javascripts/main.js"],
    "bundle": false
  },
  "fonts": {
    "files": "fonts/**/*.*",
    "destination": "assets/fonts/"
  },
  "webpack": "webpack.config.js",
  "builds": [
    {
      "format": "html",
      "destination": "build/web"
    }
  ],
  "codesplit": {
    "includes": "examples"
  },
  "layout": "layouts/default.html"
}
