{
  "title": "Code of Music",
  "addPlugins": ["magicbook-codesplit", "magicbook-webpack", "src/plugins/p5"],
  "files": ["src/notion-docs/**/*.md"],
  "images": {
    "files": [
      "src/notion-docs/images/**/*.png",
      "src/images/**/*.png",
      "src/examples/img/*.*"
    ],
    "destination": "assets/img/"
  },
  "liquid": {
    "includes": "src/includes"
  },
  "stylesheets": {
    "bundle": true,
    "compress": true,
    "digest": true,
    "files": ["src/stylesheets/html.scss", "src/stylesheets/highlightjs.css"]
  },
  "javascripts": {
    "files": ["src/javascripts/main.js"],
    "bundle": false
  },
  "fonts": {
    "files": "src/fonts/**/*.*",
    "destination": "assets/fonts/"
  },
  "webpack": "webpack.config.js",
  "builds": [
    {
      "format": "html",
      "destination": "build/web"
    }
  ],
  "codesplit": {
    "includes": "src/examples"
  },
  "layout": "src/layouts/default.html"
}
