{
  "title": "Code of Music",
  "addPlugins": [
    "magicbook-codesplit",
    "magicbook-webpack",
    "src/plugins/p5",
    "src/plugins/inlineButton",
    "src/plugins/interactiveSketch"
  ],
  "files": ["src/notion-docs/**/*.md","src/AR/pdfinteractive.html"],
  "images": {
    "files": [
      "src/notion-docs/images/**/*.png",
      "src/images/**/*.*",
      "src/examples/img/*.*"
    ],
    "destination": "assets/img/"
  },
  "stylesheets": {
    "bundle": true,
    "compress": true,
    "digest": true,
    "files": [
      "src/stylesheets/html.scss",
      "src/stylesheets/highlightjs.css",
      "src/examples/assets/examples.css"
    ]
  },
  "liquid": {
    "includes": "src/includes"
  },
  "javascripts": {
    "files": ["src/javascripts/utils.js", "src/javascripts/main.js"],
    "bundle": false
  },
  "fonts": {
    "files": "src/fonts/**/*.*",
    "destination": "assets/fonts/"
  },
  "webpack": "webpack.config.js",
  "builds": [
    {
      "format": "html",
      "destination": "build/web"
    }
  ],
  "codesplit": {
    "includes": "src/examples"
  },
  "layout": "src/layouts/default.html"
}
